<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Age_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>Age__c &lt; 0</errorConditionFormula>
    <errorDisplayField>Age__c</errorDisplayField>
    <errorMessage>Age can not be less than 0</errorMessage>
</ValidationRule>
